{
  "name": "aeon-dashboard",
  "version": "1.0.0",
  "description": "AEON Digital Twin Cybersecurity Dashboard",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:deployment": "jest tests/deployment --verbose",
    "test:health": "jest tests/deployment/health-check.test.ts --verbose",
    "test:connectivity": "jest tests/deployment/service-connectivity.test.ts --verbose",
    "test:env": "jest tests/deployment/environment-validation.test.ts --verbose",
    "validate:deployment": "npm run test:env && npm run test:connectivity && npm run test:health",
    "worker": "node scripts/start-worker.js",
    "monitor": "node scripts/queue-monitor.js",
    "redis:start": "docker-compose -f docker-compose.redis.yml up -d",
    "redis:stop": "docker-compose -f docker-compose.redis.yml down"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.62",
    "@clerk/nextjs": "^6.34.2",
    "@neondatabase/serverless": "^1.0.2",
    "@qdrant/js-client-rest": "^1.15.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@tremor/react": "^3.18.3",
    "@types/uuid": "^10.0.0",
    "ai": "^5.0.87",
    "apexcharts": "^5.3.6",
    "bullmq": "^5.63.0",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "ioredis": "^5.8.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.454.0",
    "minio": "^8.0.1",
    "mysql2": "^3.11.3",
    "neo4j-driver": "^6.0.0",
    "neovis.js": "^2.1.0",
    "next": "^15.0.3",
    "next-auth": "^4.24.10",
    "openai": "^6.8.0",
    "react": "^19.0.0",
    "react-apexcharts": "^1.8.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "recharts": "^2.13.3",
    "tailwind-merge": "^2.6.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.56.1",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/minio": "^7.1.0",
    "@types/node": "^22.9.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.3",
    "jest": "^30.2.0",
    "postcss": "^8.4.47",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
