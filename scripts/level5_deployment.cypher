// Create Relationships
// PUBLISHES: ThreatFeed -> InformationEvent

MATCH (tf:ThreatFeed)
MATCH (ie:InformationEvent)
WITH tf, ie, rand() as r
WHERE r < 0.33
MERGE (tf)-[pub:PUBLISHES {
    created: datetime(),
    reliability: tf.reliability,
    latency: tf.latency
}]->(ie);

// ACTIVATES_BIAS: InformationEvent -> CognitiveBias

MATCH (ie:InformationEvent)
WHERE ie.fearRealityGap > 2.0
MATCH (cb:CognitiveBias)
WITH ie, cb, rand() as r
WHERE r < 0.2
MERGE (ie)-[ab:ACTIVATES_BIAS {
    created: datetime(),
    activationStrength: ie.fearRealityGap / 10.0,
    trigger: 'fear_reality_gap'
}]->(cb);

// AFFECTS_SECTOR: InformationEvent -> Sector

MATCH (ie:InformationEvent)
UNWIND ie.affectedSectors as sectorName
MATCH (s:Sector)
WHERE s.name = sectorName OR s.sector = sectorName
MERGE (ie)-[aff:AFFECTS_SECTOR {
    created: datetime(),
    severity: ie.severity,
    impact: ie.cvssScore / 10.0
}]->(s);

// PROCESSES_EVENT: EventProcessor -> Events

MATCH (ep:EventProcessor)
MATCH (e:Event)
WHERE e:InformationEvent OR e:GeopoliticalEvent
WITH ep, e, rand() as r
WHERE r < 0.1
MERGE (ep)-[proc:PROCESSES_EVENT {
    created: datetime(),
    processingType: ep.processorType,
    latency: ep.latency
}]->(e);


// Validation Queries
MATCH (n:InformationEvent) RETURN 'InformationEvents' as type, count(n) as count;
MATCH (n:GeopoliticalEvent) RETURN 'GeopoliticalEvents' as type, count(n) as count;
MATCH (n:ThreatFeed) RETURN 'ThreatFeeds' as type, count(n) as count;
MATCH (n:CognitiveBias) RETURN 'CognitiveBiases' as type, count(n) as count;
MATCH (n:EventProcessor) RETURN 'EventProcessors' as type, count(n) as count;
MATCH (n:Level5) RETURN 'Total Level5' as type, count(n) as count;